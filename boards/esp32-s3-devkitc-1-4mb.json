{
  "name": "ESP32-S3-DevKitC-1 (4MB Flash)",
  "url": "https://www.espressif.com/en/products/devkits/esp32-s3-devkitc-1",
  "vendor": "Espressif",

  "build": {
    "core": "esp32",
    "mcu": "esp32s3",
    "variant": "esp32s3",
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "dio",
    "arduino": {
      "ldscript": "esp32s3_out.ld",
      "partitions": "default_littlefs.csv"
    }
  },

  "frameworks": ["arduino"],

  "connectivity": ["wifi", "bluetooth"],

  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 327680,
    "maximum_size": 4194304,
    "require_upload_port": true,
    "speed": 115200
  },

  "debug": {
    "openocd_board": "esp32s3-builtin.cfg"
  }
}
